<?xml version="1.0" ?>
<robot name="robot">
    <!-- Links-->
    <!-- Link: base_link -->
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 0.7" />
            </material>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="10.0"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.23125" ixy="0" ixz="0" iyy="0.23125" iyz="0" izz="0.3125"/>
        </inertial>
    </link>

    <!-- Link: base_footprint -->
    <link name="base_footprint">
        <visual>
            <geometry>
                <cylinder radius="0.0" length="0.0"/>
                <origin xyz="0 0 0" rpy="0 0 0"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.0" length="0.0"/>
            </geometry>
        </collision>
    </link>

    <!-- Link: front_laser -->
    <link name="front_laser">
        <visual>
            <geometry>
                <box size="0.025 0.025 0.025" />
                <origin xyz="0 0 0" rpy="0 0 0"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <box size="0.025 0.025 0.025" />
            </geometry>
        </collision>
    </link>

<!-- Main Wheels -->
    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="7.25"/>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
            <inertia ixx="0.00264322916666667" ixy="0" ixz="0" iyy="0.00264322916666667" iyz="0" izz="0.002265625"/>
        </inertial>
    </link>
    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="7.25"/>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
            <inertia ixx="0.00264322916666667" ixy="0" ixz="0" iyy="0.00264322916666667" iyz="0" izz="0.002265625"/>
        </inertial>
    </link>

    <!-- Caster Wheels -->
    <link name="front_caster">
        <visual>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>        
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.25"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.0000625" ixy="0" ixz="0" iyy="0.0000625" iyz="0" izz="0.0000625"/>
        </inertial>
    </link>

    <link name="back_caster">
        <visual>
            <geometry>
                <sphere radius="0.025"/>
                <!-- Radius = 2.5 cm -->
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.25"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.0000625" ixy="0" ixz="0" iyy="0.0000625" iyz="0" izz="0.0000625"/>
        </inertial>
    </link>

    
    <!-- Joints -->
    <!-- base_footprint joint -->
    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
    </joint>

    <!-- front_laser joint -->
    <joint name="front_laser_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_laser"/>
        <origin xyz="0.25 0.0 0.075" rpy="3.14 0 0"/>
    </joint>

    <!-- Left wheel joint -->
    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.2 -0.15" rpy="0 0 0"/>
        <axis rpy="0 0 0" xyz="0 1 0">
            <spring_reference>0</spring_reference>
            <spring_stiffness>8000.0</spring_stiffness>
            <damping>5.0</damping>
        </axis>
        <limit effort="1000.0" velocity="1000.0"/>
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Right wheel joint -->
    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.2 -0.15" rpy="0 0 0"/>
        <axis rpy="0 0 0" xyz="0 1 0">
            <spring_reference>0</spring_reference>
            <spring_stiffness>8000.0</spring_stiffness>
            <damping>5.0</damping>
        </axis>
        <limit effort="1000.0" velocity="1000.0"/>
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Front caster joint -->
    <joint name="front_caster_joint" type="continuous">
        <parent link="base_link"/>
        <child link="front_caster"/>
        <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
        <axis rpy="0 0 0" xyz="0 1 0">
            <spring_reference>0</spring_reference>
            <spring_stiffness>8000.0</spring_stiffness>
            <damping>5.0</damping>
        </axis>
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- back caster joint -->
    <joint name="back_caster_joint" type="continuous">
        <parent link="base_link"/>
        <child link="back_caster"/>
        <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
        <axis rpy="0 0 0" xyz="0 1 0">
            <spring_reference>0</spring_reference>
            <spring_stiffness>8000.0</spring_stiffness>
            <damping>5.0</damping>
        </axis>
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

<!-- Gazebo -->

    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
        <!-- <gravity>0</gravity> -->
    </gazebo>

    <gazebo reference="front_laser">
        <material>Gazebo/Red</material>
    </gazebo>

  <gazebo reference="left_wheel">
    <material>Gazebo/Grey</material>
    <mu1>2</mu1>
    <mu2>2</mu2>
    <kp>15000.0</kp>
    <kd>150.0</kd>
    <fdir1>1 0 0</fdir1>
    <minDepth>0.0001</minDepth>
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo reference="right_wheel">
    <material>Gazebo/Grey</material>
    <mu1>2</mu1>
    <mu2>2</mu2>
    <kp>15000.0</kp>
    <kd>150.0</kd>
    <fdir1>1 0 0</fdir1>
    <minDepth>0.0001</minDepth>
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo reference="front_caster">
    <mu1>0.0</mu1>
    <mu2>0.0</mu2>
    <kp>15000.0</kp>
    <kd>150.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo reference="back_caster">
    <mu1>0.0</mu1>
    <mu2>0.0</mu2>
    <kp>15000.0</kp>
    <kd>150.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
    <material>Gazebo/FlatBlack</material>
  </gazebo>



<!-- Plugins -->
    <!-- Differential Drive Plugin -->
    <gazebo>
    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <rosDebugLevel>na</rosDebugLevel>
      <alwaysOn>true</alwaysOn>
      <updateRate>50</updateRate>
      <leftJoint>left_wheel_joint</leftJoint>
      <rightJoint>right_wheel_joint</rightJoint>
      <wheelSeparation>0.4</wheelSeparation>
      <wheelDiameter>0.05</wheelDiameter>
      <wheelTorque>20</wheelTorque>
      <commandTopic>/cmd_vel</commandTopic>
      <odometryTopic>/odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <publishWheelTF>false</publishWheelTF>
      <publishOdomTF>true</publishOdomTF>
      <robotBaseFrame>base_link</robotBaseFrame>
      <publishWheelJointState>true</publishWheelJointState>
      <wheelAcceleration>true</wheelAcceleration>
      <odometrySource>world</odometrySource>
      <publishTf>true</publishTf>
    </plugin>
  </gazebo>

     <!-- Laser Scanner Plugin -->
    <gazebo reference="front_laser">
      <sensor type="ray" name="laser_sensor">
        <pose>0 0 0 0 0 0</pose>
        <visualize>true</visualize>
        <update_rate>50</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>720</samples>
              <resolution>1</resolution>
              <min_angle>-1.5708</min_angle>
              <max_angle>1.5708</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.1</min>
            <max>30.0</max>
          </range>
        </ray>
        <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
          <robotNamespace>/</robotNamespace>
          <topicName>/scan</topicName>
          <frameName>front_laser</frameName>
        </plugin>
      </sensor>
    </gazebo>


</robot>